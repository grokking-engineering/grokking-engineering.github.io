---
title: Tech Talks
---

<div class="techtalks-page">

  <%- data.events.techtalks.each do |tt| %>

    <%- if tt.incoming == true %>
      <div class="container techtalk panel">
        <h1>Incoming</h1>
        <h1>TechTalk #<%= tt.number %>: <%= tt.title %></h1>
      </div>
    <%- else %>
      <div class="container techtalk panel">
        <h1> TechTalk #<%= tt.number %>: <%= tt.title %></h1>
        <%- if tt.date %>
          <h2><%= tt.date %></h2>
        <%- end -%>

        <%- if tt.image_path %>
          <img class="img-responsive center-block img-fluid" src="<%= tt.image_path %>">
        <%- end %>
        <br/>

        <h3>Talks:</h3>
        <ul>
          <%- tt.sessions.each do |session| %>
            <li>
              <strong>
                <%= session.title %>
              </strong><%- if session.speaker %> - <%= session.speaker %> <%- end %>
              <%- if session.slide_href %> - <a href="<%= session.slide_href %>">Slide</a> <%- end %>
            </li>
          <%- end %>
        </ul>
        <h3>Info:</h3>
        <ul>
          <%- if tt.sponsor %>
            <li><strong>Sponsor: </strong><%= tt.sponsor %> </li>
          <%- end %>

          <%- if tt.venue %>
            <li><strong>Venue Sponsor: </strong><%= tt.venue %> </li>
          <%- end %>

          <%- if tt.event_href %>
            <li><strong>Event: </strong><a href="<%= tt.event_href %>">Facebook</a></li>
          <%- end %>

          <%- if tt.photo_href %>
            <li><strong>Photo: </strong><a href="<%= tt.photo_href %>">Album</a></li>
          <%- end %>
        </ul>
      </div>
    <%- end %>
  <%- end %>
</div>
